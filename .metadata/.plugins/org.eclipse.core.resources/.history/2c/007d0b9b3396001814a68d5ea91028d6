
public class Person {
	
	private int age;
	private String name;
	
	public void display()
	{
		System.out.println("name: " +name+ "\t" +"age: " + age);
	}
	
	public Person()
	{
		age=10;
		name="John Doe";
	}
	
	public Person(int a,String n)
	{
		age=a;
		name=n;
	}
	
	public void setAge(int a)
	{
		age = a;
	}

}
