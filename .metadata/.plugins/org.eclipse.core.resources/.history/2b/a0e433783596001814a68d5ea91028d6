
public class Person {
	
	private int age;
	private String name;
	
	public void display()
	{
		System.out.println("name: " +name+ "\t" +"age: " + age);
	}
	
	public Person()
	{
		age=10;
		name="John Doe";
	}
	
	public Person(int age,String name)
	{
		this.age=age;
		this.name=name;
	}
	
	public void setAge(int a)
	{
		age = a;
	}
	
	public void setName(String n)
	{
		name = n;
	}

}
